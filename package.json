{
  "name": "erulean-tools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://windrose-saga.github.io/erulean-tools/",
  "scripts": {
    "dev": "vite --port=3001",
    "build": "tsc && vite build",
